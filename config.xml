<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.firebase.test" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>FirebaseTest</name>
    <description>
        Tests a custom FCM receiver class with cordova-plugin-firebase
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-firebasex" spec="^3.0.1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="mailto:*" />
    <platform name="android">
    </platform>
    <engine name="android" spec="^8.0.0" />
</widget>
